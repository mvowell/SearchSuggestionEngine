<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox id="root" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="10.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="529.0" minWidth="10.0" prefWidth="467.0" />
          <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="303.0" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="303.0" minWidth="10.0" prefWidth="33.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button id="searchButton" fx:id="searchButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="98.0" text="Search" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Button>
            <TextField id="searchBar" fx:id="searchBar" prefHeight="26.0" prefWidth="481.0" />
            <Button id="loadFileButton" fx:id="loadFileButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="..." GridPane.columnIndex="2" />
         </children>
      </GridPane>
      <ListView id="searchSuggestionList" fx:id="searchSuggestionList" prefHeight="328.0" prefWidth="620.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
